{
  "name": "pr-bot",
  "version": "1.0.0",
  "license": "MIT",
  "main": "app/app.js",
  "scripts": {
    "start": "npx prisma generate && yarn db:migrate && ts-node -r tsconfig-paths/register app/app.ts",
    "dev": "nodemon -e ts -w ./app -x ts-node -r tsconfig-paths/register app/app.ts",
    "lint": "eslint app/**/*.ts",
    "format": "eslint app/**/*.ts --fix",
    "db:migrate": "npx prisma migrate deploy --preview-feature",
    "db:init": "npx prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@prisma/client": "^3.4.1",
    "@slack/events-api": "^3.0.1",
    "@slack/interactive-messages": "^2.0.2",
    "@slack/web-api": "^6.5.1",
    "@types/dotenv-flow": "^3.2.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "prisma": "3.4.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
